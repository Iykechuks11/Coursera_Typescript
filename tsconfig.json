{
  "compilerOptions": {
    // "target": "es6", 
    // "module": "CommonJS", 
    // "target": "ES2015",
    // "module": "es6",
    "target": "es2015",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "removeComments": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "strict": true, 
    "skipLibCheck": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true
  }
}